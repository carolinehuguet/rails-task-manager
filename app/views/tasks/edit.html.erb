<h1>Modify <%= @task.title %> </h1>

<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
<br>
<br>
  <%= f.label :details %>
  <%= f.text_field :details %>
<br>
<br>
<%= f.label :completed %>
<%= f.check_box :completed %>
<br>
<br>
  <%= f.submit %>
<% end %>


<p><%= link_to "back", tasks_path %></p>
